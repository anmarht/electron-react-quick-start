{
  "name": "electron-react-quick-start",
  "productName": "Electron React Quick Start",
  "putTogetherBy": "https://github.com/anmarht",
  "license": "CC0-1.0",
  "version": "1.0.0",
  "description": "React on ElectronJS Quick Start",
  "main": "main.js",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^13.5.0",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-scripts": "5.0.1",
    "wait-on": "^6.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "concurrently --kill-others-on-fail \"cross-env BROWSER=none npm run start-react\" \"npm:start-electron-wait\"",
    "start-electron-wait": "wait-on tcp:3000 && electron .",
    "start-react": "react-scripts start",
    "build": "react-scripts build",
    "pre-publish": "copyfiles package.json main.js node_modules/electron-is-dev/* ./build",
    "publish": "npm run pre-publish && electron-packager ./build --all --asar --out publish",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Memcached",
    "Client"
  ],
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "electron": "^18.0.3",
    "electron-packager": "^15.4.0",
    "serve": "^13.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
